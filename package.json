{
  "name": "nodebb-plugin-random-topics",
  "version": "1.0.0",
  "description": "A plugin to display random topics from all categories",
  "main": "library.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/NodeBB/nodebb-plugin-random-topics"
  },
  "keywords": [
    "nodebb",
    "plugin",
    "random",
    "topics"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "author": {
    "name": "barisusakli",
    "email": "baris@nodebb.org"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/NodeBB/nodebb-plugin-random-topics/issues"
  },
  "readmeFilename": "README.md",
  "nbbpm": {
    "compatibility": "^1.17.0"
  },
  "dependencies": {
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@commitlint/cli": "13.1.0",
    "@commitlint/config-angular": "13.1.0",
    "eslint": "7.32.0",
    "eslint-config-nodebb": "0.0.2",
    "eslint-plugin-import": "2.24.2",
    "husky": "7.0.2",
    "lint-staged": "11.1.2"
  }
}
